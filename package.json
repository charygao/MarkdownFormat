{
  "name": "markdownformat",
  "displayName": "MarkdownFormat",
  "description": "MarkdownFormat",
  "version": "0.0.1",
  "publisher": "liushilive",
  "engines": {
    "vscode": "^1.21.0"
  },
  "categories": [
    "Languages",
    "Formatters",
    "Other"
  ],
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.reFormat",
        "title": "MarkdownFormat : 格式化MD"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "extension.reFormat",
          "when": "editorLangId == markdown",
          "group": "navigation"
        }
      ]
    },
    "configuration": [
      {
        "title": "",
        "properties": {}
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.6",
    "tslint": "^5.8.0",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  }
}